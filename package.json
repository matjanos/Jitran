{
  "name": "jitrans",
  "version": "0.1.0",
  "description": "An Electron-based translator",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc & cp index.html ./dist/ & cp -R ./css ./dist & cp -R ./ico ./dist & cp -R ./font ./dist",
    "prestart": "npm run build",
    "start": "electron .",
    "dist-win32-x64": "npm run build && electron-packager . --out=dist --platform=win32 --arch=x64 --overwrite",
    "dist-all": "npm run build && electron-packager . --out=dist --platform=all --arch=all --overwrite"
  },
  "repository": {
    "type": "git",
    "url": "git+TODO"
  },
  "keywords": [],
  "author": "Kuba Matjanowski",
  "license": "MIT",
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "devDependencies": {
    "electron": "1.4.5",
    "electron-packager": "^7.0.1",
    "rivets": "^0.9.6",
    "tsc": "^1.20150623.0",
    "typescript": "^2.0.10"
  },
  "dependencies": {
    "@types/electron": "^1.4.27",
    "@types/menubar": "^5.1.2",
    "@types/node": "^6.0.48",
    "jquery-textfill": "^0.6.0",
    "menubar": "^5.1.0"
  }
}
